<div class="span4">


<div class="row">
  <div class="span3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label "注册邮箱" %>
      <%= f.text_field :email %>

      <%= f.label "密码" %>
      <%= f.password_field :password %>

      
  </div>
  <div class="span3">
      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <%= f.submit "登录", class: "btn btn-large btn-primary" %>
      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      
    <% end %>
    <%=render "users/form"%>
  </div>
 
</div>

</div>